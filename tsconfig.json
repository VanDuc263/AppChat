{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,

    "module": "esnext",
    "moduleResolution": "node",

    "jsx": "react-jsx",
    "strict": false,

    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,

    "verbatimModuleSyntax": false
  },
  "include": ["src"]
}
